{
    "/api/*": {
        "target": "https://devportalpy.senatics.gov.py/portalpy-backend-core",
        "secure": false,
        "loglevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
            "^/api": "/rest"
        }
    },
    "/plugins/*": {
        "target": "https://devportalpy.senatics.gov.py/portal-py-plugins",
        "secure": false,
        "loglevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
            "^/plugins": "/rest"
        }
    },
    "/auth/*": {
        "target": "https://devportalpy.senatics.gov.py/auth",
        "secure": false,
        "loglevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
            "^/auth": "/rest"
        }
    },
    "/docsapi/*": {
        "target": "https://devportalpy.senatics.gov.py/documentos-core",
        "secure": false,
        "loglevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
            "^/docsapi": "/rest"
        }
    }
}